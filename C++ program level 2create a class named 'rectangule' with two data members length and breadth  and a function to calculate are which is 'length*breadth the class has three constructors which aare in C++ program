#include <iostream>

class Rectangle {
private:
    double length;
    double breadth;

public:
    Rectangle() : length(0), breadth(0) {}
    Rectangle(double l, double b) : length(l), breadth(b) {}
    Rectangle(const Rectangle &rect) : length(rect.length), breadth(rect.breadth) {}
    double calculateArea() {
        return length * breadth;
    }
};

int main() {
    Rectangle rect1;
    Rectangle rect2(5, 10); 
    Rectangle rect3 = rect2; 

    std::cout << "Area of rect1: " << rect1.calculateArea() << std::endl;
    std::cout << "Area of rect2: " << rect2.calculateArea() << std::endl;
    std::cout << "Area of rect3: " << rect3.calculateArea() << std::endl;

    return 0;
}
